/* component/list/list.wxss */
.question-list {
  border-radius: 15rpx 15rpx 0px 0px;
  overflow: hidden;
}
.item {
  margin-bottom: 10rpx;
}
.loading {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.empty-bottom {
  position: absolute;
  bottom: 10%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.feedback {
  width: 457rpx;
  height: 60rpx;
  opacity: 0.5;
  border-radius: 0px 15px 15px;
  background-color: rgb(217, 0, 27);
}
.feedback2 {
  position: relative;
  left: 3rpx;
  top: 3rpx;
  width: 457rpx;
  height: 60rpx;
  border-radius: 0px 15px 15px;
  background-color: rgba(255, 255, 0, 0.804);
}
.feedback3 {
  position: relative;
  top: 5rpx;
  left: 5rpx;
  width: 473rpx;
  height: 60rpx;
  border-radius: 0px 55px 55px;
  background-color: rgb(16, 48, 224);
  font-family: Arial-Black;
  font-size: 30rpx;
  font-weight: 900;
  line-height: 60rpx;
  text-align: center;
  color: rgb(255, 255, 255);
}
.canNotFind {
  width: 300rpx;
  height: 300rpx;
  margin-left: 50%;
  margin-top: 50%;
  transform: translateX(-50%);
}
.subscript {
  margin-right: 30rpx;
  margin-top: 30rpx;
}